{
  // "paths": ["src/**"],
  "exclude": ["example"],
  "compilerOptions": {
    "target": "ES2019",                                  
    "module": "nodenext",                               
    "sourceMap": true,                                
    "outDir": "./out",                                 
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,           
    "strict": true,                                    
    "skipLibCheck": true,                               
    "noImplicitAny": false,
    "jsx": "preserve"
  }
}
